<div id="rtspmain">
<p>
  <% console.log("rtsp : ", rtsp[0])%>
  <% if(rtsp[0].url != "" ) {%>
    <% for(var i = 0 ; i < rtsp.length ; i++) {%>
      <button onclick="taportsp(this.id,this.value)" id="<%= rtsp[i].url %>"  value="<%= rtsp[i].arduinoIp%>"><%= rtsp[i].name%></button>
    <%}%>
  <%}%>
</p>
  <div>
    <canvas id="canvas"></canvas>
    <div id="label-container1"></div>
    <div id="label-container2"></div>
  </div>
</div>
